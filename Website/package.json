{
  "name": "SpaceIRS",
  "version": "1.0.0",
  "author": "Team ;3",
  "scripts": {
    "tailwatch": "npx tailwindcss -i ./src/tailwind.css -o ./src/assets/tailout.css --watch",
    "host": "npx tailwindcss -i ./src/tailwind.css -o ./src/assets/tailout.css && http-server -p 8082",
    "dev": "concurrently --kill-others \"npm run tailwatch\" \"npm run host\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "http-server": "^14.1.1",
    "jquery": "^3.7.1"
  }
}